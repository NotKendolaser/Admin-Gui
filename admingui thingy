			Button1 = "Okay...";
		})
		setclipboard(97161295)
end
	if target ~= nil and workspace[target] and workspace[target]:FindFirstChild("Humanoid") and workspace[target].Humanoid.Health > 0 and workspace[target]:FindFirstChildOfClass("Tool") and game.Players.LocalPlayer.Character:FindFirstChild("Gear Cloner") then
		local args = {
			[1] = workspace[target]:FindFirstChildOfClass("Tool")
		}
		
		game:GetService("Players").LocalPlayer.Character:FindFirstChild("Gear Cloner").GearRequest:FireServer(unpack(args))

	elseif workspace[target]:FindFirstChildOfClass("Tool") == nil and game.Players.LocalPlayer.Character:FindFirstChild("Gear Cloner") then
		game.StarterGui:SetCore("SendNotification",  {
			Title = "Unable to find gear.";
			Text = "Target doesnt have tool equipped.";
			Icon = "rbxassetid://6035067826";
			Duration = 5;
			Button1 = "Okay...";
		})
end
if target ~= nil and workspace[target] and workspace[target]:FindFirstChild("Humanoid") and workspace[target].Humanoid.Health > 0 and workspace[target]:FindFirstChildOfClass("Tool") and game.Players.LocalPlayer.Backpack:FindFirstChild("Gear Cloner") then
	game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("Gear Cloner"))
	repeat wait() until game.Players.LocalPlayer.Character:FindFirstChild("Gear Cloner")
	local args = {
		[1] = workspace[target]:FindFirstChildOfClass("Tool")
	}
	
	game:GetService("Players").LocalPlayer.Character:FindFirstChild("Gear Cloner").GearRequest:FireServer(unpack(args))

elseif workspace[target]:FindFirstChildOfClass("Tool") == nil and game.Players.LocalPlayer.Character:FindFirstChild("Gear Cloner") then
	game.StarterGui:SetCore("SendNotification",  {
		Title = "Unable to find gear.";
		Text = "Target doesnt have tool equipped.";
		Icon = "rbxassetid://6035067826";
		Duration = 5;
		Button1 = "Okay...";
	})
end
end)

