    ExploitSection:NewLabel("Gear Blacklister")

    ExploitSection:NewButton("Activate", "Activate gear blacklister", function()
        notify()
        local Gears = {
            "Frozen Scroll Of Sevenless",
            "TriLaserGun",
            "ScytheOfSingularity",
            "RainbowTriLaserGun",
            "Thunderbolt",
            "LightningHorn",
            "FuchsiaPeriastron",
            "KOSSGustHorn",
            "BlizzardWand",
            "StopHittingYourself",
            "SledgeHammer",
            "StaffOfAzureEverIce",
            "StaffOfCrimsonChaosFire",
            "Technological staff",
            "FreezeRay",
            "ScrollOfSevenless"
        }
        game:GetService("RunService").Heartbeat:Connect(function()
            pcall(function()
                for i,v in pairs(game.Players:GetPlayers()) do
                    if v ~= game.Players.LocalPlayer then
                        _G.gearfound = false
                        for e, gear in pairs(v.Character:GetChildren()) do
                            if gear:IsA("Tool") then
                                for _, g in pairs(Gears) do
                                    if g == gear.Name then
                                        _G.gearfound = true
                                    end
                                end
                            end
                        end
                        if _G.gearfound == true and v.Character.Humanoid.Health ~= 0 then
                            local args = {
                                [1] = v.Character.HumanoidRootPart.Position - Vector3.new(0,1,0),
                                [2] = v.Character.HumanoidRootPart.Position
                            }
        
                            game:GetService("Players").LocalPlayer.Backpack.RocketJumper.FireRocket:FireServer(unpack(args))
                            local ohNumber1 = 7
                            local ohInstance2 = v.Character.Humanoid
                            local ohNumber3 = 9999999
        
                            game.Players.LocalPlayer.Backpack["Diamond Blade Sword"].GearScript.Controller:InvokeServer(ohNumber1, ohInstance2, ohNumber3)
                        end
                    end
                end
            end)
        end)
    end)


