			Title = "Unable to find gear.";
			Text = "The gear id has been set to your clipboard (268586231)";
			Icon = "rbxassetid://6035067826";
			Duration = 5;
			Button1 = "Okay...";
		})
		setclipboard(268586231)
	end

	if game.Players.LocalPlayer.Character:FindFirstChild("AzureDragonMagicSlayerSword") then
		game:GetService("Players").LocalPlayer.Character.AzureDragonMagicSlayerSword.Server.SkeleScript.Idle:Destroy()
	end
	if game.Players.LocalPlayer.Backpack:FindFirstChild("AzureDragonMagicSlayerSword") then
		game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("AzureDragonMagicSlayerSword"))
		game:GetService("Players").LocalPlayer.Character.AzureDragonMagicSlayerSword.Server.SkeleScript.Idle:Destroy()
end
end)

