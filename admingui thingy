--Exploits stuffs

ExploitSection:NewLabel("Bat Knight Bat Sword")

ExploitSection:NewButton("Gives Forcefield", "Gives you a forcefield", function()
	notify()
	local function myoldhumanoid()
		game.Players.LocalPlayer.Character.Humanoid.AutoRotate = true
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
		end
		
		if game.Players.LocalPlayer.Character:FindFirstChild("BatKnightBatSword") then
			game:GetService("Players").LocalPlayer.Character.BatKnightBatSword.Server.BatTransform.DisableBackpack:Destroy()	
			local args = {[1] = Enum.KeyCode.Q}
			game:GetService("Players").LocalPlayer.Character.BatKnightBatSword.Remote:FireServer(unpack(args))
			repeat wait() until game.Players.LocalPlayer.Character.Humanoid.AutoRotate == false and game.Players.LocalPlayer.Character.Humanoid.JumpPower == 0
			myoldhumanoid()
		end
		
		
		if game.Players.LocalPlayer.Backpack:FindFirstChild("BatKnightBatSword") then
			game:GetService("Players").LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("BatKnightBatSword"))
			if game.Players.LocalPlayer.Character:FindFirstChild("BatKnightBatSword") then
			game:GetService("Players").LocalPlayer.Character.BatKnightBatSword.Server.BatTransform.DisableBackpack:Destroy()
				local args = {[1] = Enum.KeyCode.Q}
				game:GetService("Players").LocalPlayer.Character.BatKnightBatSword.Remote:FireServer(unpack(args))
				repeat wait() until game.Players.LocalPlayer.Character.Humanoid.AutoRotate == false and game.Players.LocalPlayer.Character.Humanoid.JumpPower == 0
				myoldhumanoid()
			end
			end
		
		
		if game.Players.LocalPlayer.Backpack:FindFirstChild("BatKnightBatSword") == nil and game.Players.LocalPlayer.Character:FindFirstChild("BatKnightBatSword") == nil then
			game.StarterGui:SetCore("SendNotification",  {
				Title = "Unable to find gear.";
				Text = "The gear id has been set to your clipboard (17680660)";
				Icon = "rbxassetid://6035067826";
				Duration = 5;
				Button1 = "Okay...";
			})
			setclipboard(17680660)
		end
	end)

	ExploitSection:NewButton("Permanent Bat", "Makes you a bat forever", function()
		notify()
	
			if game.Players.LocalPlayer.Character:FindFirstChild("BatKnightBatSword") then
				game:GetService("Players").LocalPlayer.Character.BatKnightBatSword.Server.BatTransform.EnableBackpack:Destroy()	
				local args = {[1] = Enum.KeyCode.Q}
				game:GetService("Players").LocalPlayer.Character.BatKnightBatSword.Remote:FireServer(unpack(args))
		
			end
			
			
			if game.Players.LocalPlayer.Backpack:FindFirstChild("BatKnightBatSword") then
				game:GetService("Players").LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("BatKnightBatSword"))
				if game.Players.LocalPlayer.Character:FindFirstChild("BatKnightBatSword") then
				game:GetService("Players").LocalPlayer.Character.BatKnightBatSword.Server.BatTransform.EnableBackpack:Destroy()
					local args = {[1] = Enum.KeyCode.Q}
					game:GetService("Players").LocalPlayer.Character.BatKnightBatSword.Remote:FireServer(unpack(args))
				end
				end
			
			if game.Players.LocalPlayer.Backpack:FindFirstChild("BatKnightBatSword") == nil and game.Players.LocalPlayer.Character:FindFirstChild("BatKnightBatSword") == nil then
				game.StarterGui:SetCore("SendNotification",  {
					Title = "Unable to find gear.";
					Text = "The gear id has been set to your clipboard (17680660)";
					Icon = "rbxassetid://6035067826";
					Duration = 5;
					Button1 = "Okay...";
				})
				setclipboard(17680660)
			end
			
	end)

ExploitSection:NewLabel("Vampire Vanquisher: Sword and Shield")

ExploitSection:NewButton("Infinite Stun + Disables Backpack (Q)", "Q ability infinite stun and disables backpack", function()
	notify()
	if game.Players.LocalPlayer.Character:FindFirstChild("VampireVanquisher") then
		game:GetService("Players").LocalPlayer.Character.VampireVanquisher.Server.Vanquish.VanquishStun.EnableBackpack:Destroy()	
	end
	
	
	
	
	if game.Players.LocalPlayer.Backpack:FindFirstChild("VampireVanquisher") then
		game:GetService("Players").LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("VampireVanquisher"))
		if game.Players.LocalPlayer.Character:FindFirstChild("VampireVanquisher") then
			game:GetService("Players").LocalPlayer.Character.VampireVanquisher.Server.Vanquish.VanquishStun.EnableBackpack:Destroy()
		end
		end
	
	
	if game.Players.LocalPlayer.Backpack:FindFirstChild("VampireVanquisher") == nil and game.Players.LocalPlayer.Character:FindFirstChild("VampireVanquisher") == nil then
		game.StarterGui:SetCore("SendNotification",  {
			Title = "Unable to find gear.";
			Text = "The gear id has been set to your clipboard (94794847)";
			Icon = "rbxassetid://6035067826";
			Duration = 5;
			Button1 = "Okay...";
		})
		setclipboard(94794847)
	end
end)

