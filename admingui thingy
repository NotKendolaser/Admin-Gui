ExploitSection:NewToggle("Spam Shards", "Spam the crystal orb shards", function(state)
	notify()
	getgenv().SpamOrb = state
	if game.Players.LocalPlayer.Character:FindFirstChild("OrbBlastgun") == nil and game.Players.LocalPlayer.Backpack:FindFirstChild("OrbBlastgun") == nil then
		game.StarterGui:SetCore("SendNotification",  {
			Title = "Unable to find gear.";
			Text = "The gear id has been set to your clipboard (61916108)";
			Icon = "rbxassetid://61916108";
			Duration = 5;
			Button1 = "Okay...";
		})
		setclipboard(61916108)
	end
        while task.wait() do
            	if not SpamOrb then return end
				if game:GetService("Players").LocalPlayer.Character:FindFirstChild("OrbBlastgun") then
					local args = {[1] = CFrame.new(Vector3.new(16.566036224365234, 0, -46.43155288696289), Vector3.new(-0.32959479093551636, -0.004598359111696482, 0.9441112875938416))}game:GetService("Players").LocalPlayer.Character.OrbBlastgun.Mouse1Down:FireServer(unpack(args))
				elseif game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("OrbBlastgun") then
					game:GetService("Players").LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("OrbBlastgun"))
					local args = {[1] = CFrame.new(Vector3.new(16.566036224365234, 0, -46.43155288696289), Vector3.new(-0.32959479093551636, -0.004598359111696482, 0.9441112875938416))}game:GetService("Players").LocalPlayer.Character.OrbBlastgun.Mouse1Down:FireServer(unpack(args))
end
end
end)

ExploitSection:NewLabel("Skull Cracker - Level Up Exclusive")

ExploitSection:NewButton("Disable Backpack", "Disables the target backpack on hit", function()
notify()
if game.Players.LocalPlayer.Character:FindFirstChild("SkullCracker") then
	game:GetService("Players").LocalPlayer.Character.SkullCracker.BasherScript.Stun.EnableBackpack:Destroy()	
end




if game.Players.LocalPlayer.Backpack:FindFirstChild("SkullCracker") then
	game:GetService("Players").LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("SkullCracker"))
	if game.Players.LocalPlayer.Character:FindFirstChild("SkullCracker") then
		game:GetService("Players").LocalPlayer.Character.SkullCracker.BasherScript.Stun.EnableBackpack:Destroy()
	end
	end


if game.Players.LocalPlayer.Backpack:FindFirstChild("SkullCracker") == nil and game.Players.LocalPlayer.Character:FindFirstChild("SkullCracker") == nil then
	game.StarterGui:SetCore("SendNotification",  {
		Title = "Unable to find gear.";
		Text = "The gear id has been set to your clipboard (65469908)";
		Icon = "rbxassetid://6035067826";
		Duration = 5;
		Button1 = "Okay...";
	})
	setclipboard(65469908)
end
end)

