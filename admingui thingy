local function BRJQQY_fake_script() -- EnableBackpack.LocalScript 
	local script = Instance.new('LocalScript', EnableBackpack)

	local button = script.Parent
	_G.BackpackEnabled = false
	local function enabletoggle()
		if button.BackgroundColor3 == Color3.fromRGB(170, 0, 0) then
			button.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
			button.Text = "Enable Backpack : ON"
			_G.BackpackEnabled = true
			pcall(function()
				while task.wait(.1) do
					if _G.BackpackEnabled == true then
						game:GetService("StarterGui"):SetCoreGuiEnabled("Backpack", true)
					elseif _G.BackpackEnabled == false then break end
				end
			end)
		end
	end
	local function disabletoggle()
		if button.BackgroundColor3 == Color3.fromRGB(0, 255, 0) then
			_G.BackpackEnabled = false
			button.BackgroundColor3 = Color3.fromRGB(170, 0, 0)
			button.Text = "Enable Backpack : OFF"
		end
	end
	button.MouseButton1Click:Connect(function()
		if _G.BackpackEnabled == false then
			enabletoggle()
		end
		if _G.BackpackEnabled == true then
			disabletoggle()
		end
	end)
end
coroutine.wrap(BRJQQY_fake_script)()
local function VCTQ_fake_script() -- EquipAll.LocalScript 
	local script = Instance.new('LocalScript', EquipAll)

	script.Parent.MouseButton1Click:Connect(function()
		for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
			if v:IsA("Tool") then
				v.Parent = game.Players.LocalPlayer.Character
			end
		end
	end)
end
