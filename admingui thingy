LocalPlayerSection:NewButton("View gear name", "See your gear name", function()
	notify()
	if game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool") then	
		game.StarterGui:SetCore("SendNotification",  {
			Title = "A gear name has copied to your clipboard";
			Text = "Gear name: "..game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").Name;
			Icon = "rbxassetid://6031082527";
			Duration = 5;
			Button1 = "Dismiss";
		})
		setclipboard(tostring(game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").Name))
	end
	if game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool") == nil then
		game.StarterGui:SetCore("SendNotification",  {
			Title = "Unable to find gear";
			Text = "Please equip gear to";
			Icon = "rbxassetid://6031082527";
			Duration = 5;
			Button1 = "Dismiss";
		})
	end
end)

LocalPlayerSection:NewLabel("Seranok's Rocket Jumper")
LocalPlayerSection:NewToggle("Rocket no cooldown", "Gives no cooldown to rocket", function(hehehe)
    notify()
	getgenv().rocketnocd = hehehe
local plr = game:GetService("Players")
local lplr = plr.LocalPlayer
local mouse = lplr:GetMouse()
local random = math.random
local UserInputService = game:GetService("UserInputService")
local shooting = true

pcall(function()
    if  game.Players.LocalPlayer.Character:FindFirstChild("RocketJumper") == nil and game.Players.LocalPlayer.Backpack:FindFirstChild("RocketJumper") == nil then
		game.StarterGui:SetCore("SendNotification",  {
			Title = "Unable to find gear.";
			Text = "The gear id has been set to your clipboard (169602103)";
			Icon = "rbxassetid://6035067826";
			Duration = 5;
			Button1 = "Okay...";
		})
		setclipboard(169602103)
end
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if not rocketnocd then return end
    if input.UserInputType == Enum.UserInputType.MouseButton1 and lplr.Character:FindFirstChild("RocketJumper") and rocketnocd then
ddedede = true
while ddedede do
    wait(0.1)
local ohVector31 = mouse.Hit.p--lplr.Character.HumanoidRootPart.Position
local ohVector32 = mouse.Hit.p
lplr.Character.RocketJumper.FireRocket:FireServer(ohVector31, ohVector32)
end
end
end)

UserInputService.InputEnded:Connect(function(input, gameProcessed)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
ddedede = false
end
end)
end)
end)

LocalPlayerSection:NewLabel("KOSS Wind Summoner")
LocalPlayerSection:NewToggle("Wind summoner no cooldown", "Gives no cooldown to wind summoner", function(lol)
    notify()
	getgenv().windsummon = lol
local plr = game:GetService("Players")
local lplr = plr.LocalPlayer
local mouse = lplr:GetMouse()
local random = math.random
local UserInputService = game:GetService("UserInputService")
local shooting = true

pcall(function()
    if  game.Players.LocalPlayer.Character:FindFirstChild("KOSSGustHorn") == nil and game.Players.LocalPlayer.Backpack:FindFirstChild("KOSSGustHorn") == nil then
		game.StarterGui:SetCore("SendNotification",  {
			Title = "Unable to find gear.";
			Text = "The gear id has been set to your clipboard (127506257)";
			Icon = "rbxassetid://6035067826";
			Duration = 5;
			Button1 = "Okay...";
		})
		setclipboard(127506257)
end
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if not windsummon then return end
    if input.UserInputType == Enum.UserInputType.MouseButton1 and lplr.Character:FindFirstChild("KOSSGustHorn") and windsummon then
shooting = true
while shooting do
    wait(0.1)
    local args = {
        [1] = "DO THE THING!!!"
    }
    
    game:GetService("Players").LocalPlayer.Character.KOSSGustHorn.RemoteEvent:FireServer(unpack(args))
    
end
end
end)

UserInputService.InputEnded:Connect(function(input, gameProcessed)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
shooting = false
end
end)
end)
end)

