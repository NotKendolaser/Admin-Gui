local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("OP Gears Gui", "DarkTheme")

local LocalPlayerTab = Window:NewTab("LocalPlayer")

local LocalPlayerSection = LocalPlayerTab:NewSection("LocalPlayer")

local ExploitTab = Window:NewTab("Exploits")

local ExploitSection = ExploitTab:NewSection("Exploits")

local TargetTab = Window:NewTab("Target")

local TargetSection = TargetTab:NewSection("Target")

local AllPlrTab = Window:NewTab("All Players")

local AllPlrSection = AllPlrTab:NewSection("All Players")

local playerlist = {}

local target

local function notify()
	game.StarterGui:SetCore("SendNotification",  {
		Title = "Executed";
		Text = "Executed the script";
		Icon = "rbxassetid://6035067842";
		Duration = 3;
		Button1 = "Dismiss";
	})
end

for i, v in next, game.Players:GetPlayers() do
   --if v ~= game.Players.LocalPlayer then
        table.insert(playerlist, v.Name)
    end
--end

--Local player stuffs
local namelabel = LocalPlayerSection:NewLabel("Name: "..game.Players.LocalPlayer.Name)
local agelabel = LocalPlayerSection:NewLabel("AccountAge: "..game.Players.LocalPlayer.AccountAge)
local wslabel = LocalPlayerSection:NewLabel("WalkSpeed: "..game.Players.LocalPlayer.Character.Humanoid.WalkSpeed)
local hplabel = LocalPlayerSection:NewLabel("Health: "..game.Players.LocalPlayer.Character.Humanoid.Health)
local maxhplabel = LocalPlayerSection:NewLabel("MaxHealth: "..game.Players.LocalPlayer.Character.Humanoid.MaxHealth)
game.Players.LocalPlayer.CharacterAdded:Connect(function(char)
    local hum = char:WaitForChild("Humanoid")
    hum:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
		wslabel:UpdateLabel("WalkSpeed: "..game.Players.LocalPlayer.Character.Humanoid.WalkSpeed)
    end)
	hum:GetPropertyChangedSignal("Health"):Connect(function()
		hplabel:UpdateLabel("Health: "..game.Players.LocalPlayer.Character.Humanoid.Health)
	end)
	hum:GetPropertyChangedSignal("MaxHealth"):Connect(function()
		hplabel:UpdateLabel("MaxHealth: "..game.Players.LocalPlayer.Character.Humanoid.MaxHealth)
	end)
end)

LocalPlayerSection:NewButton("Immune to :explode", "Removes HumanoidRootPart", function()
	notify()
    if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
		local char = game:GetService("Players").LocalPlayer.Character
char.Parent = nil
char.HumanoidRootPart:Destroy()
char.Parent = workspace
	end
end)

LocalPlayerSection:NewButton("BreakJoints Left Arm", "BreakJoints Left Arm", function()
	notify()
    if game.Players.LocalPlayer.Character:FindFirstChild("Left Arm") then
		game.Players.LocalPlayer.Character["Left Arm"]:BreakJoints()
	end
end)

LocalPlayerSection:NewButton("Delete holding tool", "Delete the tool that you are holding", function()
	notify()
if game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool") then
		game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool"):Destroy()
	end
end)

LocalPlayerSection:NewButton("Equip All Tools", "Equip all tools in backpack", function()
	notify()
    for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
		if v:IsA("Tool") then
			v.Parent = game.Players.LocalPlayer.Character
		end
	end
end)


LocalPlayerSection:NewButton("Delete all tools that are equipped", "Delete all tools in character", function()
	notify()
for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
		if v:IsA("Tool") then
			v:Destroy()
		end
	end
end)

LocalPlayerSection:NewButton("Infinite Yield", "Loads up infinite yield", function()
	notify()
loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

LocalPlayerSection:NewButton("Freeze", "Freeze your character", function()
	notify()
game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
end)

LocalPlayerSection:NewButton("Thaw", "Thaw your character", function()
	notify()
game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
end)

LocalPlayerSection:NewButton("Joins & Leaves Notifier", "Tell you who joins server or leave", function()
	notify()
local debris = game:GetService("Debris")
	game.Players.PlayerAdded:Connect(function(player)
		local hoho = Instance.new("Sound", game.Workspace)
		hoho.Volume = 1.5
		hoho.SoundId = "rbxassetid://1523818134"
		game.StarterGui:SetCore("SendNotification",  {
			Title = "A Player has joined.";
			Text = "DisplayName: "..player.DisplayName .. " Name: "..player.Name .. " AccountAge: "..player.AccountAge .." Days";
			Icon = "rbxassetid://6031082527";
			Duration = 5;
			Button1 = "Dismiss";
		})
		hoho:Play()
		debris:AddItem(hoho, 3)
	end)

	game.Players.PlayerRemoving:Connect(function(player)
		local hehe = Instance.new("Sound", game.Workspace)
		hehe.Volume = 1.5
		hehe.SoundId = "rbxassetid://1523818134"
		game.StarterGui:SetCore("SendNotification",  {
			Title = "A Player has left.";
			Text = "DisplayName: "..player.DisplayName .. " Name: "..player.Name .. " AccountAge: "..player.AccountAge .." Days";
			Icon = "rbxassetid://6031082522";
			Duration = 5;
			Button1 = "Dismiss";
		})
		hehe:Play()
		debris:AddItem(hehe, 3)
	end)
	end)
 

LocalPlayerSection:NewToggle("Enable Backpack", "Always Enable Backpack", function(state)
	notify()
	getgenv().Backpack = state
	pcall(function()
        while task.wait(.1) do
            	if not Backpack then return end
				print("test")
					game:GetService("StarterGui"):SetCoreGuiEnabled("Backpack", true)
end
end)
end)

LocalPlayerSection:NewToggle("Invis all tools", "Invis all of your tools", function(state)
	notify()
	getgenv().invistools = state
	for i, a in pairs(game.Players.LocalPlayer.Backpack:GetDescendants()) do
		if a:IsA("BasePart") then
			a.Transparency = 1
		end
end
	pcall(function()
        while task.wait(.1) do
            	if not invistools then return end
				for i, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
					if v:IsA("Tool") then
						for i, _ in pairs(v:GetDescendants()) do
							if _:IsA("BasePart") then
						   _.Transparency = 1     
							end
						end
					end
				end
end
end)
end)

LocalPlayerSection:NewButton("Rejoin", "Rejoin server", function()
	notify()
	local ts = game:GetService("TeleportService")

	local p = game:GetService("Players").LocalPlayer
	
	 
	
	ts:Teleport(game.PlaceId, p)
end)

