--Exploits stuffs

ExploitSection:NewLabel("Bat Knight Bat Sword")

ExploitSection:NewButton("Gives Forcefield", "Gives you a forcefield", function()
	notify()
	local function myoldhumanoid()
		game.Players.LocalPlayer.Character.Humanoid.AutoRotate = true
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
		end
		
		if game.Players.LocalPlayer.Character:FindFirstChild("BatKnightBatSword") then
			game:GetService("Players").LocalPlayer.Character.BatKnightBatSword.Server.BatTransform.DisableBackpack:Destroy()	
			local args = {[1] = Enum.KeyCode.Q}
			game:GetService("Players").LocalPlayer.Character.BatKnightBatSword.Remote:FireServer(unpack(args))
			repeat wait() until game.Players.LocalPlayer.Character.Humanoid.AutoRotate == false and game.Players.LocalPlayer.Character.Humanoid.JumpPower == 0
			myoldhumanoid()
		end
		
		
		if game.Players.LocalPlayer.Backpack:FindFirstChild("BatKnightBatSword") then
			game:GetService("Players").LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("BatKnightBatSword"))
			if game.Players.LocalPlayer.Character:FindFirstChild("BatKnightBatSword") then
			game:GetService("Players").LocalPlayer.Character.BatKnightBatSword.Server.BatTransform.DisableBackpack:Destroy()
				local args = {[1] = Enum.KeyCode.Q}
				game:GetService("Players").LocalPlayer.Character.BatKnightBatSword.Remote:FireServer(unpack(args))
				repeat wait() until game.Players.LocalPlayer.Character.Humanoid.AutoRotate == false and game.Players.LocalPlayer.Character.Humanoid.JumpPower == 0
				myoldhumanoid()
			end
			end
		
		
		if game.Players.LocalPlayer.Backpack:FindFirstChild("BatKnightBatSword") == nil and game.Players.LocalPlayer.Character:FindFirstChild("BatKnightBatSword") == nil then
			game.StarterGui:SetCore("SendNotification",  {
				Title = "Unable to find gear.";
				Text = "The gear id has been set to your clipboard (17680660)";
				Icon = "rbxassetid://6035067826";
				Duration = 5;
				Button1 = "Okay...";
			})
			setclipboard(17680660)
		end
	end)

	ExploitSection:NewButton("Permanent Bat", "Makes you a bat forever", function()
		notify()
	
			if game.Players.LocalPlayer.Character:FindFirstChild("BatKnightBatSword") then
				game:GetService("Players").LocalPlayer.Character.BatKnightBatSword.Server.BatTransform.EnableBackpack:Destroy()	
				local args = {[1] = Enum.KeyCode.Q}
				game:GetService("Players").LocalPlayer.Character.BatKnightBatSword.Remote:FireServer(unpack(args))
		
			end
			
			
			if game.Players.LocalPlayer.Backpack:FindFirstChild("BatKnightBatSword") then
				game:GetService("Players").LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("BatKnightBatSword"))
				if game.Players.LocalPlayer.Character:FindFirstChild("BatKnightBatSword") then
				game:GetService("Players").LocalPlayer.Character.BatKnightBatSword.Server.BatTransform.EnableBackpack:Destroy()
					local args = {[1] = Enum.KeyCode.Q}
					game:GetService("Players").LocalPlayer.Character.BatKnightBatSword.Remote:FireServer(unpack(args))
				end
				end
			
			if game.Players.LocalPlayer.Backpack:FindFirstChild("BatKnightBatSword") == nil and game.Players.LocalPlayer.Character:FindFirstChild("BatKnightBatSword") == nil then
				game.StarterGui:SetCore("SendNotification",  {
					Title = "Unable to find gear.";
					Text = "The gear id has been set to your clipboard (17680660)";
					Icon = "rbxassetid://6035067826";
					Duration = 5;
					Button1 = "Okay...";
				})
				setclipboard(17680660)
			end
			
	end)

ExploitSection:NewLabel("Vampire Vanquisher: Sword and Shield")

ExploitSection:NewButton("Infinite Stun + Disables Backpack (Q)", "Q ability infinite stun and disables backpack", function()
	notify()
	if game.Players.LocalPlayer.Character:FindFirstChild("VampireVanquisher") then
		game:GetService("Players").LocalPlayer.Character.VampireVanquisher.Server.Vanquish.VanquishStun.EnableBackpack:Destroy()	
	end
	
	
	
	
	if game.Players.LocalPlayer.Backpack:FindFirstChild("VampireVanquisher") then
		game:GetService("Players").LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("VampireVanquisher"))
		if game.Players.LocalPlayer.Character:FindFirstChild("VampireVanquisher") then
			game:GetService("Players").LocalPlayer.Character.VampireVanquisher.Server.Vanquish.VanquishStun.EnableBackpack:Destroy()
		end
		end
	
	
	if game.Players.LocalPlayer.Backpack:FindFirstChild("VampireVanquisher") == nil and game.Players.LocalPlayer.Character:FindFirstChild("VampireVanquisher") == nil then
		game.StarterGui:SetCore("SendNotification",  {
			Title = "Unable to find gear.";
			Text = "The gear id has been set to your clipboard (94794847)";
			Icon = "rbxassetid://6035067826";
			Duration = 5;
			Button1 = "Okay...";
		})
		setclipboard(94794847)
	end
end)

ExploitSection:NewButton("Glitch Target Camera", "Glitch the target camera who got killed by your sword", function()
	notify()
	if game.Players.LocalPlayer.Character:FindFirstChild("VampireVanquisher") then
		game:GetService("Players").LocalPlayer.Character.VampireVanquisher.Server.Finisher.Finish.EnableBackpack:Destroy()
	end




	if game.Players.LocalPlayer.Backpack:FindFirstChild("VampireVanquisher") then
		game:GetService("Players").LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("VampireVanquisher"))
		if game.Players.LocalPlayer.Character:FindFirstChild("VampireVanquisher") then
			game:GetService("Players").LocalPlayer.Character.VampireVanquisher.Server.Finisher.Finish.EnableBackpack:Destroy()
		end
	end


	if game.Players.LocalPlayer.Backpack:FindFirstChild("VampireVanquisher") == nil and game.Players.LocalPlayer.Character:FindFirstChild("VampireVanquisher") == nil then
		game.StarterGui:SetCore("SendNotification",  {
			Title = "Unable to find gear.";
			Text = "The gear id has been set to your clipboard (94794847)";
			Icon = "rbxassetid://6035067826";
			Duration = 5;
			Button1 = "Okay...";
		})
		setclipboard(94794847)
	end
end)

ExploitSection:NewButton("Crash server", "Crash the server", function()
	notify()
	local lplr = game.Players.LocalPlayer

	local function crash()
		pcall(function()
		while task.wait() do
				game.Players.LocalPlayer.Character["Left Arm"].ArmWeld:Destroy()
			end
		end)
	end

	if lplr.Character:FindFirstChild("VampireVanquisher") then
		crash()
	end

	if lplr.Backpack:FindFirstChild("VampireVanquisher") then
		lplr.Character.Humanoid:EquipTool(lplr.Backpack:FindFirstChild("VampireVanquisher"))
		if lplr.Character:FindFirstChild("VampireVanquisher") then
			crash()
		end
	end

	if game.Players.LocalPlayer.Backpack:FindFirstChild("VampireVanquisher") == nil and game.Players.LocalPlayer.Character:FindFirstChild("VampireVanquisher") == nil then
		game.StarterGui:SetCore("SendNotification",  {
			Title = "Unable to find gear.";
			Text = "The gear id has been set to your clipboard (94794847)";
			Icon = "rbxassetid://6035067826";
			Duration = 5;
			Button1 = "Okay...";
		})
		setclipboard(94794847)
	end

	if game.Players.LocalPlayer.Character:FindFirstChild("VampireVanquisher") and not game.Players.LocalPlayer.Character["Left Arm"]:FindFirstChild("ArmWeld") then
		game.StarterGui:SetCore("SendNotification",  {
			Title = "Crash Failed";
			Text = "The game uses another version of vampire vanquisher";
			Icon = "rbxassetid://6035067826";
			Duration = 5;
			Button1 = "Okay...";
		})
		lplr.Character:FindFirstChild("VampireVanquisher"):Destroy()
	end

end)

ExploitSection:NewLabel("Orb Crystal Launcher")

ExploitSection:NewToggle("Spam Shards", "Spam the crystal orb shards", function(state)
	notify()
	getgenv().SpamOrb = state
	if game.Players.LocalPlayer.Character:FindFirstChild("OrbBlastgun") == nil and game.Players.LocalPlayer.Backpack:FindFirstChild("OrbBlastgun") == nil then
		game.StarterGui:SetCore("SendNotification",  {
			Title = "Unable to find gear.";
			Text = "The gear id has been set to your clipboard (61916108)";
			Icon = "rbxassetid://61916108";
			Duration = 5;
			Button1 = "Okay...";
		})
		setclipboard(61916108)
	end
        while task.wait() do
            	if not SpamOrb then return end
				if game:GetService("Players").LocalPlayer.Character:FindFirstChild("OrbBlastgun") then
					local args = {[1] = CFrame.new(Vector3.new(16.566036224365234, 0, -46.43155288696289), Vector3.new(-0.32959479093551636, -0.004598359111696482, 0.9441112875938416))}game:GetService("Players").LocalPlayer.Character.OrbBlastgun.Mouse1Down:FireServer(unpack(args))
				elseif game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("OrbBlastgun") then
					game:GetService("Players").LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("OrbBlastgun"))
					local args = {[1] = CFrame.new(Vector3.new(16.566036224365234, 0, -46.43155288696289), Vector3.new(-0.32959479093551636, -0.004598359111696482, 0.9441112875938416))}game:GetService("Players").LocalPlayer.Character.OrbBlastgun.Mouse1Down:FireServer(unpack(args))
end
end
end)

ExploitSection:NewLabel("Skull Cracker - Level Up Exclusive")

ExploitSection:NewButton("Disable Backpack", "Disables the target backpack on hit", function()
notify()
if game.Players.LocalPlayer.Character:FindFirstChild("SkullCracker") then
	game:GetService("Players").LocalPlayer.Character.SkullCracker.BasherScript.Stun.EnableBackpack:Destroy()	
end




if game.Players.LocalPlayer.Backpack:FindFirstChild("SkullCracker") then
	game:GetService("Players").LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("SkullCracker"))
	if game.Players.LocalPlayer.Character:FindFirstChild("SkullCracker") then
		game:GetService("Players").LocalPlayer.Character.SkullCracker.BasherScript.Stun.EnableBackpack:Destroy()
	end
	end


if game.Players.LocalPlayer.Backpack:FindFirstChild("SkullCracker") == nil and game.Players.LocalPlayer.Character:FindFirstChild("SkullCracker") == nil then
	game.StarterGui:SetCore("SendNotification",  {
		Title = "Unable to find gear.";
		Text = "The gear id has been set to your clipboard (65469908)";
		Icon = "rbxassetid://6035067826";
		Duration = 5;
		Button1 = "Okay...";
	})
	setclipboard(65469908)
end
end)

