-- if target ~= nil and workspace[target].Character and workspace[target].Character:FindFirstChild("Humanoid") and workspace[target].Character.Humanoid.Health > 0 then

TargetSection:NewToggle("LoopBring Player", "LoopBring player to you (Client sided)", function(hey)
	getgenv().loop = hey
	notify()
	local hum = game.Players.LocalPlayer.Character.HumanoidRootPart
	pcall(function()
	while task.wait() do
		if not loop then return end
	if target ~= nil and workspace[target] and workspace[target]:FindFirstChild("Humanoid") and workspace[target].Humanoid.Health > 0 then
workspace[target].HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(2,1,0)
	end
end
end)
end)

TargetSection:NewLabel("Seranok's Rocket Jumper")

TargetSection:NewToggle("Spam Rocket", "Spam Rocket to player", function(lol)
	getgenv().rocket = lol
	notify()
	local hum = game.Players.LocalPlayer.Character.HumanoidRootPart
	if  game.Players.LocalPlayer.Character:FindFirstChild("RocketJumper") == nil and game.Players.LocalPlayer.Backpack:FindFirstChild("RocketJumper") == nil then
		game.StarterGui:SetCore("SendNotification",  {
			Title = "Unable to find gear.";
			Text = "The gear id has been set to your clipboard (169602103)";
			Icon = "rbxassetid://6035067826";
			Duration = 5;
			Button1 = "Okay...";
		})
		setclipboard(169602103)
end
pcall(function()
	while task.wait(.1) do
		if not rocket then return end
	if target ~= nil and workspace[target] and workspace[target]:FindFirstChild("Humanoid") and workspace[target].Humanoid.Health > 0 and game.Players.LocalPlayer.Character:FindFirstChild("RocketJumper") then
		local args = {
			[1] = workspace[target].Torso.Position - Vector3.new(0,1,0),
			[2] = workspace[target].Torso.Position
		}

		game:GetService("Players").LocalPlayer.Character.RocketJumper.FireRocket:FireServer(unpack(args))
	end
	if target ~= nil and workspace[target] and workspace[target]:FindFirstChild("Humanoid") and workspace[target].Humanoid.Health > 0 and game.Players.LocalPlayer.Backpack:FindFirstChild("RocketJumper") then
		local args = {
			[1] = workspace[target].Torso.Position + Vector3.new(0,1,0),
			[2] = workspace[target].Torso.Position
		}

		game:GetService("Players").LocalPlayer.Backpack.RocketJumper.FireRocket:FireServer(unpack(args))
end
end
end)
end)

TargetSection:NewLabel("Diamond Blade Sword")

TargetSection:NewToggle("Kill Player", "Kill player", function(lol)
	getgenv().crystal = lol
	notify()
	if  game.Players.LocalPlayer.Character:FindFirstChild("Diamond Blade Sword") == nil and game.Players.LocalPlayer.Backpack:FindFirstChild("Diamond Blade Sword") == nil then
		game.StarterGui:SetCore("SendNotification",  {
			Title = "Unable to find gear.";
			Text = "The gear id has been set to your clipboard (173755801)";
			Icon = "rbxassetid://6035067826";
			Duration = 5;
			Button1 = "Okay...";
		})
		setclipboard(169602103)
end
pcall(function()
	while task.wait(.1) do
		if not crystal then return end
	if target ~= nil and workspace[target] and workspace[target]:FindFirstChild("Humanoid") and workspace[target].Humanoid.Health > 0 and game.Players.LocalPlayer.Character:FindFirstChild("Diamond Blade Sword") then
		local args = {
			[1] = 7,
			[2] = workspace[target].Humanoid,
			[3] = tonumber("inf")
		}
		
		game:GetService("Players").LocalPlayer.Character:FindFirstChild("Diamond Blade Sword").GearScript.Controller:InvokeServer(unpack(args))
	end

	if target ~= nil and workspace[target] and workspace[target]:FindFirstChild("Humanoid") and workspace[target].Humanoid.Health > 0 and game.Players.LocalPlayer.Backpack:FindFirstChild("Diamond Blade Sword") then
		local args = {
			[1] = 7,
			[2] = workspace[target].Humanoid,
			[3] = tonumber("inf")
		}
		
		game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Diamond Blade Sword").GearScript.Controller:InvokeServer(unpack(args))
end
end
end)
end)

TargetSection:NewLabel("Stop Hitting Yourself")

TargetSection:NewToggle("Loop Hit Player", "Loop using the laser to player", function(lol)
	getgenv().laser = lol
	notify()
	if  game.Players.LocalPlayer.Character:FindFirstChild("StopHittingYourself") == nil and game.Players.LocalPlayer.Backpack:FindFirstChild("StopHittingYourself") == nil then
		game.StarterGui:SetCore("SendNotification",  {
			Title = "Unable to find gear.";
			Text = "The gear id has been set to your clipboard (173755801)";
			Icon = "rbxassetid://6035067826";
			Duration = 5;
			Button1 = "Okay...";
		})
		setclipboard(169602103)
end
pcall(function()
	while task.wait(.1) do
		if not laser then return end
	if target ~= nil and workspace[target] and workspace[target]:FindFirstChild("Humanoid") and workspace[target].Humanoid.Health > 0 and game.Players.LocalPlayer.Character:FindFirstChild("StopHittingYourself") then
		local args = {
			[1] = workspace[target].Head
		}
		
		game:GetService("Players").LocalPlayer.Character.StopHittingYourself.MouseClick:FireServer(unpack(args))
		
	end

	if target ~= nil and workspace[target] and workspace[target]:FindFirstChild("Humanoid") and workspace[target].Humanoid.Health > 0 and game.Players.LocalPlayer.Backpack:FindFirstChild("StopHittingYourself") then
		game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("StopHittingYourself"))
		local args = {
			[1] = workspace[target].Head
		}
		
		game:GetService("Players").LocalPlayer.Character.StopHittingYourself.MouseClick:FireServer(unpack(args))
end
end
end)
end)

TargetSection:NewTextBox("Spam Command", "Spam command here", function(txt)
	notify()
	while task.wait(3) do
	local args = {
		[1] = txt
	}
	
	game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(unpack(args))
end
end)


-- Refresh PlayerList
game.Players.PlayerAdded:Connect(function(player)
    for i, v in next, playerlist do
        table.remove(playerlist, i)
    end
    for i, plrs in next, game.Players:GetPlayers() do
        if plrs ~= game.Players.LocalPlayer then
            insert = true
            for _, v in next, playerlist do
                if v == plrs.Name then
                    insert = false
                end
            end
            if insert then
                table.insert(playerlist, plrs.Name)
            end
        end
    end
    targetdropdown:Refresh(playerlist)
end)

game.Players.PlayerRemoving:Connect(function(player)
    for i, v in next, playerlist do
        table.remove(playerlist, i)
    end
    for i, plrs in next, game.Players:GetPlayers() do
        if plrs ~= game.Players.LocalPlayer then
            insert = true
            for _, v in next, playerlist do
                if v == plrs.Name then
                    insert = false
                end
            end
            if insert then
                table.insert(playerlist, plrs.Name)
            end
        end
    end
    targetdropdown:Refresh(playerlist)
end)
