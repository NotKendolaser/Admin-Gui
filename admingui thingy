local function FRHVUJE_fake_script() -- startthemachine.LocalScript 
	local script = Instance.new('LocalScript', startthemachine)

	local button = script.Parent
	_G.StartSpam = false
	local function enabletoggle()
		if button.BackgroundColor3 == Color3.fromRGB(0, 255, 0) then
			button.BackgroundColor3 = Color3.fromRGB(170, 0, 0)
			button.Text = "STOP"
			_G.StartSpam = true
			pcall(function()
				while task.wait(tonumber(script.Parent.Parent.Delay.Text)) do
					if _G.StartSpam == true then
						game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(tostring(script.Parent.Parent.CommandTarget.Text))
					elseif _G.StartSpam == false then break end
				end
			end)
		end
	end
	local function disabletoggle()
		if button.BackgroundColor3 == Color3.fromRGB(170, 0, 0) then
			_G.StartSpam = false
			button.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
			button.Text = "START"
		end
	end
	button.MouseButton1Click:Connect(function()
		if _G.StartSpam == false then
			enabletoggle()
		end
		if _G.StartSpam == true then
			disabletoggle()
		end
	end)
end
